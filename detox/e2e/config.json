{
  "setupFilesAfterEnv": ["./test/setup.ts"],
  "maxWorkers": 1,
  "testEnvironment": "./environment",
  "testRunner": "jest-circus/runner",
  "testTimeout": 120000,
  "testRegex": "\\.e2e\\.ts$",
  "transform": {
    "\\.ts?$": "ts-jest"
  },
  "reporters": ["detox/runners/jest/streamlineReporter"],
  "verbose": true,
  "moduleNameMapper": {
    "^@support/(.*)": "<rootDir>/support/$1"
  }
}
